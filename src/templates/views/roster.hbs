<!DOCTYPE html>
<html>
<head>
	<title>{{title}}</title>

	<link rel="stylesheet" type="text/css" href="/css/roster-styles.css">
</head>
<body>
    {{>header}}
	<div class="main-content">
        <h2>{{title}}</h2>
        <form class="brother-search">
            <input class="search" type="text" placeholder="Search for brother...">
            <button class="search-button">Search</button>
        </form>
        
		{{#each users}}
            <div class="card">
                <img src="/brothers/{{id}}/avatar">
                <h4 id="name">{{firstName}} {{lastName}}</h4>
                <p id="email">{{email}}</p>
                <p>{{field}}</p>
            </div>
        {{/each}}
	</div>
		
<script type="text/javascript" src="/js/roster.js">
    //window.onload()
</script>
</body>
</html>